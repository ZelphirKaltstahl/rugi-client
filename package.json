{
    "name": "rugi-client",
    "version": "0.1.0",
    "description": "This is a web client and Shogi UI using the Rugi microservice for move validation.",
    "main": "lib/index.js",
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
        "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
    ],
    "scripts": {
        "clean": "rimraf lib && rimraf web/dist && rimraf example/lib",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepublish": "npm run build",
        "build": "npm run clean && tsc",
        "build:web": "npm run build && cd web && webpack",
        "docs": "typedoc src --out docs",
        "watch": "npm run clean && tsc --watch",
        "watch:web": "npm run build && cd web && webpack --watch"
    },
    "keywords": [
        "shogi",
        "client",
        "UI"
    ],
    "author": "Zelphir Kaltstahl",
    "license": "GPL-3.0",
    "repository": "git@github.com:ZelphirKaltstahl/rugi-client.git",
    "dependencies": {
        "@phosphor/widgets": "^1.5.0",
        "@phosphor/datagrid": "^0.1.5",
        "@phosphor/messaging": "^1.2.2"
    },
    "devDependencies": {
        "typedoc": "^0.11.1",
        "css-loader": "^0.28.10",
        "typescript": "^2.8",
        "rimraf": "^2.6.2",
        "file-loader": "^0.11.1",
        "rimraf": "^2.5.2",
        "style-loader": "^0.13.1",
        "webpack": "^4.4.1"
    }
}
